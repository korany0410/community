<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="post">
	<insert id="insert">
		INSERT INTO post VALUES(seq_post_idx.nextVal, #{title}, #{content}, #{username}, #{category},
		#{sub_category}, sysdate, 0, 0, 0)
	</insert>
</mapper>












